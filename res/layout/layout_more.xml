<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    			xmlns:android="http://schemas.android.com/apk/res/android"
			    android:layout_width="match_parent"
			    android:layout_height="match_parent"
			    android:orientation="vertical" 
			    >
		<include 	layout="@layout/header_more"
					android:layout_width="fill_parent"
					android:layout_height="50dp"
					/>
		<ScrollView 
		    		android:layout_width="fill_parent"
		    		android:layout_height="fill_parent"	
		    		>
		    			
		    <LinearLayout
		    			android:layout_width="fill_parent"
		    			android:layout_height="wrap_content"
		    			android:orientation="vertical"
		    			android:padding="10dp"
		    			>
    			
		        
		        
		        	<LinearLayout 
								android:layout_width="fill_parent"
								android:layout_height="wrap_content"
								android:background="@drawable/iphone_top_line_bg"
								android:layout_marginTop="20dp"
								/>
						<LinearLayout 
									 android:id="@+id/linearAbountOfMore"
									 android:layout_width="fill_parent"
									 android:layout_height="wrap_content"
									 android:orientation="horizontal"
									 android:background="@drawable/iphone_table_lne_bg"
									 android:padding="5dp"
									  >	
									
									    	<ImageView 
												    		android:layout_width="30dp"
												    		android:layout_height="30dp"
												    		android:src="@drawable/more_about"
												    		android:scaleType="fitXY"
												    		/>
												<LinearLayout 
															android:layout_width="fill_parent"
															android:layout_height="fill_parent"
															android:gravity="center"
															android:layout_weight="1"
															>
														<TextView 
																  android:text="@string/str_about"
																  android:layout_width="fill_parent"
																  android:layout_height="fill_parent"
																  android:gravity="center_vertical|left"
																  style="@style/RecordText"
																  />
												</LinearLayout>
												<TextView 
												    		android:layout_width="20dp"
												    		android:layout_height="fill_parent"
												    		android:text=">"
												    		android:layout_marginBottom="5dp"
												    		 style="@style/arrow_right"
												    		/>
												
						</LinearLayout>
						<LinearLayout 
								    android:layout_width="fill_parent"
								    android:layout_height="wrap_content"
								    android:background="@drawable/iphone_bottom_line_bg"
								    />
						
				    <LinearLayout 
								android:layout_width="fill_parent"
								android:layout_height="wrap_content"
								android:background="@drawable/iphone_top_line_bg"
								android:layout_marginTop="30dp"
								/>
					<LinearLayout 
								 android:layout_width="fill_parent"
								 android:layout_height="wrap_content"
								 android:orientation="vertical"
								 android:background="@drawable/iphone_table_lne_bg"
								  >	
							
							<LinearLayout 
								    	android:id="@+id/linearMapOfMore"
								    	android:layout_width="fill_parent"
								    	android:layout_height="wrap_content"
								    	android:orientation="horizontal"
								    	android:padding="5dp"
								    	android:baselineAligned="false"
								    	android:gravity="center_vertical"
								    	>
								    	<ImageView 
											    		android:layout_width="30dp"
											    		android:layout_height="30dp"
											    		android:src="@drawable/more_communicate"
											    		/>
											<LinearLayout 
														android:layout_width="fill_parent"
														android:layout_height="fill_parent"
														android:gravity="center"
														android:layout_weight="1"
														>
													<TextView 
															  android:text="@string/str_more_map"
															  android:layout_width="fill_parent"
															  android:layout_height="fill_parent"
															  android:gravity="center_vertical|left"
															  style="@style/RecordText"
															  />
											</LinearLayout>
											<TextView 
											    		android:layout_width="20dp"
											    		android:layout_height="fill_parent"
											    		android:text=">"
											    		android:layout_marginBottom="5dp"
											    		 style="@style/arrow_right"
											    		/>
											
								</LinearLayout>
						</LinearLayout>
						
						<LinearLayout 
								    android:layout_width="fill_parent"
								    android:layout_height="wrap_content"
								    android:background="@drawable/iphone_bottom_line_bg"
								    />
						
						
						<LinearLayout 
								android:layout_width="fill_parent"
								android:layout_height="wrap_content"
								android:background="@drawable/iphone_top_line_bg"
								android:layout_marginTop="30dp"
								/>
						<LinearLayout 
									 android:id="@+id/linearContactOfMore"
									 android:layout_width="fill_parent"
									 android:layout_height="wrap_content"
									 android:orientation="horizontal"
									 android:background="@drawable/iphone_table_lne_bg"
									 android:padding="5dp"
									  >	
									
									    	<ImageView 
												    		android:layout_width="30dp"
												    		android:layout_height="30dp"
												    		android:src="@drawable/more_contact"
												    		/>
												<LinearLayout 
															android:layout_width="fill_parent"
															android:layout_height="fill_parent"
															android:gravity="center"
															android:layout_weight="1"
															>
														<TextView 
																  android:text="@string/str_more_contact"
																  android:layout_width="fill_parent"
																  android:layout_height="fill_parent"
																  android:gravity="center_vertical|left"
																  style="@style/RecordText"
																  />
												</LinearLayout>
												<TextView 
												    		android:layout_width="20dp"
												    		android:layout_height="fill_parent"
												    		android:text=">"
												    		android:layout_marginBottom="5dp"
												    		 style="@style/arrow_right"
												    		/>
												
						</LinearLayout>
						<LinearLayout 
								    android:layout_width="fill_parent"
								    android:layout_height="wrap_content"
								    android:background="@drawable/iphone_bottom_line_bg"
								    />
						
						<LinearLayout 
								android:layout_width="fill_parent"
								android:layout_height="wrap_content"
								android:background="@drawable/iphone_top_line_bg"
								android:layout_marginTop="30dp"
								/>
						<LinearLayout 
									 android:id="@+id/linearVersionOfMore"
									 android:layout_width="fill_parent"
									 android:layout_height="wrap_content"
									 android:orientation="horizontal"
									 android:background="@drawable/iphone_table_lne_bg"
									 android:padding="5dp"
									  >	
									
									    	<ImageView 
												    		android:layout_width="30dp"
												    		android:layout_height="30dp"
												    		android:src="@drawable/more_version"
												    		/>
												<LinearLayout 
															android:layout_width="fill_parent"
															android:layout_height="fill_parent"
															android:gravity="center"
															android:layout_weight="1"
															>
														<TextView 
																  android:text="@string/str_more_version"
																  android:layout_width="fill_parent"
																  android:layout_height="fill_parent"
																  android:gravity="center_vertical|left"
																  style="@style/RecordText"
																  />
												</LinearLayout>
												<TextView 
												    		android:layout_width="20dp"
												    		android:layout_height="fill_parent"
												    		android:text=">"
												    		android:layout_marginBottom="5dp"
												    		 style="@style/arrow_right"
												    		/>
						</LinearLayout>
						<LinearLayout 
								    android:layout_width="fill_parent"
								    android:layout_height="wrap_content"
								    android:background="@drawable/iphone_bottom_line_bg"
								    />
						<LinearLayout 
								android:layout_width="fill_parent"
								android:layout_height="wrap_content"
								android:background="@drawable/iphone_top_line_bg"
								android:layout_marginTop="30dp"
								/>
						<LinearLayout 
									 android:id="@+id/linearZhoubianOfMore"
									 android:layout_width="fill_parent"
									 android:layout_height="wrap_content"
									 android:orientation="horizontal"
									 android:background="@drawable/iphone_table_lne_bg"
									 android:padding="5dp"
									  >	
									    	<ImageView 
												    		android:layout_width="30dp"
												    		android:layout_height="30dp"
												    		android:src="@drawable/more_icon_zhoubian"
												    		/>
												<LinearLayout 
															android:layout_width="fill_parent"
															android:layout_height="fill_parent"
															android:gravity="center"
															android:layout_weight="1"
															>
														<TextView 
																  android:text="@string/str_more_zhoubian"
																  android:layout_width="fill_parent"
																  android:layout_height="fill_parent"
																  android:gravity="center_vertical|left"
																  style="@style/RecordText"
																  />
												</LinearLayout>
												<TextView 
												    		android:layout_width="20dp"
												    		android:layout_height="fill_parent"
												    		android:text=">"
												    		android:layout_marginBottom="5dp"
												    		 style="@style/arrow_right"
												    		/>
						</LinearLayout>
						<LinearLayout 
								    android:layout_width="fill_parent"
								    android:layout_height="wrap_content"
								    android:background="@drawable/iphone_bottom_line_bg"
								    />
				</LinearLayout>
		</ScrollView>
</LinearLayout>